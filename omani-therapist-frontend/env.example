# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000
NEXT_PUBLIC_WEBSOCKET_URL=ws://localhost:8000/ws

# Application Settings
NEXT_PUBLIC_APP_NAME=Therapist Assistant
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_ENVIRONMENT=development

# Feature Flags
NEXT_PUBLIC_ENABLE_VOICE_INTERFACE=true
NEXT_PUBLIC_ENABLE_CBT_TOOLS=true
NEXT_PUBLIC_ENABLE_CRISIS_INTERVENTION=true
NEXT_PUBLIC_ENABLE_EMOTIONAL_ANALYSIS=true

# Voice Interface Settings
NEXT_PUBLIC_VOICE_RECOGNITION_LANGUAGE=en-US
NEXT_PUBLIC_VOICE_SYNTHESIS_LANGUAGE=en-US
NEXT_PUBLIC_VOICE_SYNTHESIS_VOICE=default

# WebSocket Configuration
NEXT_PUBLIC_WEBSOCKET_RECONNECT_ATTEMPTS=5
NEXT_PUBLIC_WEBSOCKET_RECONNECT_DELAY=1000

# Session Management
NEXT_PUBLIC_SESSION_TIMEOUT=3600000
NEXT_PUBLIC_MAX_SESSION_DURATION=7200000

# Analytics (Optional)
NEXT_PUBLIC_ANALYTICS_ID=
NEXT_PUBLIC_ANALYTICS_ENABLED=false

# Error Reporting (Optional)
NEXT_PUBLIC_ERROR_REPORTING_ENABLED=false
NEXT_PUBLIC_ERROR_REPORTING_URL=

# Development Settings
NEXT_PUBLIC_DEBUG_MODE=false
NEXT_PUBLIC_LOG_LEVEL=info

# Language Settings
NEXT_PUBLIC_DEFAULT_LANGUAGE=en
NEXT_PUBLIC_SUPPORTED_LANGUAGES=en,ar

# UI Configuration
NEXT_PUBLIC_THEME=light
NEXT_PUBLIC_ANIMATIONS_ENABLED=true 